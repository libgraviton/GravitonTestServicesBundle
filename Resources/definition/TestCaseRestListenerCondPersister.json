{
  "id": "TestCaseRestListenerCondPersister",
  "service": {
    "readOnly": false,
    "routerBase": "/testcase/rest-listeners-cond-persister/",
    "listeners": [
      {
        "serviceName": "Graviton\\RestBundle\\RestListener\\ConditionalRestrictionPersisterListener",
        "events": ["prePersist"],
        "calls": [
          {
            "method": "setEntityName",
            "arguments": [
              "Hans"
            ]
          },
          {
            "method": "setFieldName",
            "arguments": [
              "hansId"
            ]
          }
        ]
      }
    ],
    "fixtures": [
      {
        "id": "1005",
        "stringField": "hans",
        "entityId": "1000"
      },
      {
        "id": "1010",
        "stringField": "fred",
        "entityId": "2000"
      },
      {
        "id": "1999",
        "stringField": "franz",
        "entityId": "3000"
      }
    ]
  },
  "target": {
    "fields": [
      {
        "name": "id",
        "type": "string"
      },
      {
        "name": "stringField",
        "type": "string"
      },
      {
        "name": "entityId",
        "type": "string"
      }
    ]
  }
}
